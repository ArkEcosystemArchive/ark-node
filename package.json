{
  "name": "ark-node",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "doc": "docco -t docs/template/arkio.jst -c docs/template/arkio.css modules/* app.js README.md",
    "start:testnet": "node app.js --genesis genesisBlock.testnet.json --config config.testnet.json",
    "start:devnet": "node app.js --genesis genesisBlock.devnet.json --config config.devnet.json",
    "start:mainnet": "node app.js --genesis genesisBlock.mainnet.json --config config.mainnet.json -i",
    "start:test": "node app.js --genesis test/genesisBlock.json --config test/config.json",
    "test": "./node_modules/.bin/mocha",
    "cov": "./node_modules/.bin/mocha --require blanket -R html-cov test/helpers test/logic > tmp/coverage.html"
  },
  "author": "Boris Povod <boris@crypti.me>, Pavel Nekrasov <landgraf.paul@gmail.com>, Oliver Beddows <oliver@lisk.io>, FX Thoorens <fx.thoorens@ark.io>",
  "dependencies": {
    "arkjs": "github:arkecosystem/ark-js#mainnet",
    "async": "=2.0.1",
    "bip39": "^2.2.0",
    "body-parser": "^1.18.3",
    "bytebuffer": "=5.0.1",
    "change-case": "=3.0.0",
    "colors": "=1.1.2",
    "commander": "=2.9.0",
    "compression": "^1.7.3",
    "cors": "=2.8.1",
    "ejs": "=2.5.5",
    "express": "^4.16.4",
    "express-domain-middleware": "=0.1.0",
    "express-query-int": "=1.0.1",
    "express-rate-limit": "=2.5.0",
    "extend": "=3.0.0",
    "ip": "=1.1.3",
    "json-schema": "=0.2.3",
    "json-sql": "LiskHQ/json-sql#27e1ed1",
    "lodash": "=4.17.5",
    "method-override": "=2.3.10",
    "pg-monitor": "=0.5.11",
    "pg-native": "=1.10.0",
    "pg-promise": "=5.3.3",
    "popsicle": "=8.2.0",
    "pug": "^2.0.0-beta11",
    "randomstring": "=1.1.5",
    "request-ip": "^2.1.1",
    "rimraf": "=2.5.4",
    "socket.io": "=1.4.8",
    "strftime": "=0.9.2",
    "valid-url": "=1.0.9",
    "validator": "=5.7.0",
    "validator.js": "=2.0.3",
    "vorpal": "^1.11.4",
    "z-schema": "=3.18.0"
  },
  "devDependencies": {
    "bitcore-mnemonic": "^1.5.0",
    "blanket": "=1.2.3",
    "browserify-bignum": "=1.3.0-2",
    "buffer": "=4.9.1",
    "chai": "=3.5.0",
    "chai-bignumber": "=2.0.0",
    "crypto-browserify": "=3.11.0",
    "csv": "=1.1.0",
    "faker": "=3.1.0",
    "grunt": "=0.4.5",
    "grunt-cli": "=1.2.0",
    "grunt-contrib-compress": "=1.3.0",
    "grunt-contrib-jshint": "=1.0.0",
    "grunt-exec": "=1.0.1",
    "grunt-jsdox": "=0.1.7",
    "grunt-obfuscator": "=0.1.0",
    "jsdox": "=0.4.10",
    "jshint": "^2.9.6",
    "mocha": "=3.1.0",
    "moment": "=2.22.2",
    "supertest": "=2.0.0"
  },
  "config": {
    "blanket": {
      "pattern": [
        "helpers",
        "logic"
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "tmp"
      ]
    }
  }
}
